{
  "include": ["**/*"],
  "exclude": ["node_modules","build.ts"],
  "compilerOptions": {
    "target": "es2023",
    "module": "es2022",
    "moduleResolution": "bundler",
    "lib": ["es2023","dom"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "incremental": false
  }
}
